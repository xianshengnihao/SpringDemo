<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">



        <!--通过构造器注入-->
        <bean id="someBean4" class="com.spring_01._09constructors.ConstructorsBean">


                <constructor-arg index="0" value="郝维格"/>
                <constructor-arg index="1" ref="otherBean2"/>
                <constructor-arg index="2" value="123456"/>
        </bean>
        <bean id="otherBean2" class="com.spring_01._09constructors.Otherbean"/>

</beans>